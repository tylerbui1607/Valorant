                                    <button class="action-btn btn-edit">
                                        <i class="far fa-edit"></i>
                                    </button>
                                </div>
                            </td>
                            <script>
                                switch ({{this.role}}) {
                                    case 0:
                                    document.querySelectorAll('.infotype')[{{this.id}} - 1].textContent = 'Admin';
                                break;
                                    case 1:
                                document.querySelectorAll('.infotype')[{{this.id}} - 1].textContent = 'Sale';
                                break;
                                    case 2:
                                document.querySelectorAll('.infotype')[{{this.id}} - 1].textContent = 'Technical';
                                break;
                                    case 3:
                                document.querySelectorAll('.infotype')[{{this.id}} - 1].textContent = 'Delivery';
                                break;
                                }
                                if ({{ this.role }}== 0) {
                                    document.querySelectorAll('.staff-data .infostatus')[{{this.id}} - 1].innerHTML =
                                    '<div class="working status">Working</div>'}
                                else {
                                    document.querySelectorAll('.staff-data .infostatus')[{{this.id}} - 1].innerHTML =
                                    '<div class="quit status">Quit</div>'}

                                if ({{this.status}}== 1) {
