                            <div class="table-header">
                                <div>
                                    Type
                                </div>
                                <div>
                                    <button class="sort-btn">
                                        <span style="color: rgb(175, 175, 175);">
                                            <i class="fas fa-sort"></i>
                                        </span>
                                    </button>
                                </div>
                            </div>
                        </th>
                        <th class="price-field">
                            <div class="table-header">
                                <div>
                                    Price
                                </div>
                                <div>
                                    <button class="sort-btn">
                                        <span style="color: rgb(175, 175, 175);">
                                            <i class="fas fa-sort"></i>
                                        </span>
                                    </button>
                                </div>
                            </div>
                        </th>
                        <th class="status-field">
                            <div class="table-header">
                                <div>
                                    Status
                                </div>
                                <div>
                                    <button class="sort-btn">
                                        <span style="color: rgb(175, 175, 175);">
                                            <i class="fas fa-sort"></i>
                                        </span>
                                    </button>
                                </div>
                            </div>
                        </th>
                        <th class="action-field">
                            <div class="table-header">
                                <div>
                                    Action
                                </div>
                                <div>
                                    <button class="sort-btn">
                                        <span style="color: rgb(175, 175, 175);">
                                            <i class="fas fa-sort"></i>
