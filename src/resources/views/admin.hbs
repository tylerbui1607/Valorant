    <div class="setting-container">
        <div class="setting-section product-section">
            <div class="add-product-container">
                <div class="status-title">
                    New Product
                </div>
                <form class="add-product-form" method="POST" autocomplete="off">
                    <div class="input-container">
                        <label for="pname">Product name:</label><br>
                        <input type="text" id="pname" name="pname"><br>
                        <p class="wrong-input-message">Please enter product name!</p>
                    </div>
                    <div class="input-container">
                        <div style="display: flex; margin-top: 0.5rem;">
                            <label for="type">Type:</label>
                            <div style="display: inline-block; margin-left: 2rem">
                                <div style="display: flex; width: 8rem; justify-content: space-between">
                                    <div>
                                        <div>
                                            <input type="radio" name="type" id="tchair" value="1">
                                            <label for="tchair">Chair</label>
                                        </div>
                                        <div>
                                            <input type="radio" name="type" id="ttable" value="2">
                                            <label for="ttable">Table</label><br>
                                        </div>
                                    </div>
                                    <div>
                                        <div>
                                            <input type="radio" name="type" id="tbed" value="4">
                                            <label for="tbed">Bed</label>
                                        </div>
                                        <div>
                                            <input type="radio" name="type" id="tdecor" value="3">
                                            <label for="tdecor">Decor</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <p class="wrong-input-message">Please select product type!</p>
                    </div>
                    <div class="input-container">
                        <label for="supplier">Supplier:</label><br>
                        <select name="supplier" id="supplier">
                            {{#each data.suppliers}}
                            <option value="{{this.id}}">{{this.name}}</option>
                            {{/each}}
                        </select><br>
                    </div>
                    <div class="input-container">
                        <label for="price">Price:</label><br>
                        <input type="number" min="0" id="price" name="price"><br>
                        <p class="wrong-input-message">Please enter product price!</p>
                    </div>
                    <div class="input-container">
                        <label for="number">Number:</label><br>
                        <input type="number" min="0" id="number" name="number" value="1"><br>
                    </div>
                    <div class="input-container">
                        <label for="image">Image:</label>
                        <input type="file" name="image" id="image"><br>
                    </div>
                    <div style="margin-top: 0.6rem; display: flex; justify-content:space-between;">
                        <input type="button" class="submit-btn" id="product-submit" value="Add Product">
                        <input type="button" class="reset-btn" value="Clear">
                    </div>
                    <a style="display: none;" id="link" download=""></a>
                </form>
            </div>
