                <div class="table-container">
                    <table>
                        <tr>
                            <th class="id-field">
                            </th>
                            <th class="name-field">
                            </th>
                            <th class="type-field">
                            </th>
                            <th class="price-field">
                            </th>
                            <th class="status-field">
                            </th>
                            <th class="action-field">
                            </th>
                        </tr>
                        {{#each data.products}}
                        <tr class="product-data" 1 status="{{this.status}}" link="{{this.image}}"
                            number="{{this.number}}" discount="{{this.discount}}" pid="{{this._id}}"
                            supplier="{{this.supplier}}" status="{{this.status}}">
                            <td class="pinfoid">{{this.id}}</td>
                            <td class="pinfoname">{{this.name}}</td>
                            <td class="pinfotype">{{this.type}}</td>
                            <td class="pinfoprice">{{this.price}}</td>
                            <td class="pinfostatus">
                            </td>
                            <script>
                                if ({{ this.status }} == 1) {
                                    document.querySelectorAll('.pinfostatus')[{{this.id}} - 1].innerHTML =
                                    '<div class="instock status">In stock</div>'}
                                    else if ({{ this.status }} == 2) {
                                    document.querySelectorAll('.pinfostatus')[{{this.id}} - 1].innerHTML =
                                    '<div class="outstock status">Out of stock</div>'}
                                    else if ({{ this.status }} == 3) {
                                    document.querySelectorAll('.pinfostatus')[{{this.id}} - 1].innerHTML =
                                    '<div class="suspension status">Suspension</div>'}
                            </script>
                            <td>
                                <div class="action">
                                    <button class="action-btn btn-edit">
                                        <i class="far fa-edit"></i>
                                    </button>
                                </div>
                            </td>
                        </tr>
                        {{/each}}
                    </table>
                </div>
            </div>
        </div>
